<Window x:Class="quikBuyGate.Settings"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:quikBuyGate"
        mc:Ignorable="d"
        Title="Settings" Height="228.4" Width="800" Loaded="Window_Loaded">
    <Grid>
        <TextBox x:Name="tbAPI" HorizontalAlignment="Left" Height="23" Margin="51,44,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="717"/>
        <TextBox x:Name="tbSecret" HorizontalAlignment="Left" Height="23" Margin="63,102,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="705"/>
        <Label Content="API" HorizontalAlignment="Left" Margin="15,41,0,0" VerticalAlignment="Top"/>
        <Label Content="Secret" HorizontalAlignment="Left" Margin="15,102,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btnSave" Content="Сохранить" HorizontalAlignment="Left" Margin="659,156,0,0" VerticalAlignment="Top" Width="75" Click="btnSave_Click"/>

    </Grid>
</Window>
